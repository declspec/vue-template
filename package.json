{
  "name": "vue-template",
  "version": "0.1.0",
  "description": "Basic SPA template with Vue + webpack + TypeScript + Sass",
  "main": "index.js",
  "scripts": {
    "start:dev": "set NODE_ENV=development&& webpack-dev-server --history-api-fallback --overlay --config ./webpack.config.js",
    "start:prod": "set NODE_ENV=production&& webpack-dev-server --history-api-fallback --overlay --config ./webpack.config.js",
    "build:dev": "set NODE_ENV=development&& webpack --config ./webpack.config.js",
    "build:prod": "set NODE_ENV=production&& webpack --config ./webpack.config.js"
  },
  "repository": {},
  "keywords": [
    "vue",
    "vue-template",
    "template"
  ],
  "author": "Jason Larke <jason.larke@curtin.edu.au>",
  "license": "MIT",
  "devDependencies": {
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^9.0.2",
    "ts-loader": "^8.0.0",
    "tslib": "^2.3.1",
    "typescript": "^3.9.6",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-loader": "^15.9.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
